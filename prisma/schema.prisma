generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  uuid    String @default(uuid()) @unique
  empresa  String
  material String
  volumen String
  fecha String
  placa String
  idCamion String
  operador String
  checador String
  createdAt DateTime @default(now())
}

model QueryView {
  id       Int      @id @default(autoincrement())
  filterName     String
  filters  String     
  userName   String
  createdAt DateTime @default(now())
}
